{
  "name": "topleader",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "latest",
    "@mui/x-date-pickers": "^5.0.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "firebase": "^9.13.0",
    "iconv-lite": "^0.6.3",
    "papaparse": "^5.4.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.41.3",
    "react-intl": "^6.2.8",
    "react-query": "^4.0.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-swipeable-views": "^0.14.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "build-github": "DISABLE_ESLINT_PLUGIN=true REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract-translations-long": "formatjs extract 'src/**/*.js*' --ignore='src/translations' --out-file extracted/en-with-descriptions.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "extract-translations-short": "formatjs extract 'src/**/*.js*' --ignore='src/translations' --out-file extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format scripts/extract-translations-formatter.js",
    "extract-translations": "npm run extract-translations-short ; npm run extract-translations-long",
    "compile": "formatjs compile",
    "translations-match-csv": "node ./scripts/translations-csv-converter.js",
    "generate-translations": "npm run extract-translations && npm run translations-match-csv",
    "convert-translations-from-windows-1250": "node ./scripts/win1250toutf8.js",
    "convert-translations-to-windows-1250": "node ./scripts/utf8towin1250.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy_bak": "https://topleader.hasura.app",
  "proxy": "https://topleader-394306.ey.r.appspot.com",
  "devDependencies": {
    "@formatjs/cli": "^6.0.1",
    "string-similarity": "^4.0.4"
  }
}